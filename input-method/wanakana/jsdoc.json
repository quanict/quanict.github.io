{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|utils|__tests__)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "gh-pages/docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    }
  },
  "docdash": {
    "sort": false
  }
}
